@model StudentItemVm
<div class="sidebar" id="sidebar">
    <div class="sidebar-inner slimscroll">
        <div id="sidebar-menu" class="sidebar-menu">
            <ul>
                <li class="menu-title">
                    <span>Main Menu</span>
                </li>
                <li class=" active">
                    <a asp-action="index" asp-controller="student">
                        <i class="feather-grid"></i> <span>Home</span>
                    </a>
                </li>
                <li class="submenu">
                    <a href="#">
                        <i class="fas fa-graduation-cap"></i> <span>Group</span> <span class="menu-arrow"></span>
                    </a>
                    <ul>
                        <li>
                            <a asp-action="group" asp-controller="student" asp-route-id="@Model.Group?.Id">@Model.Group?.Name @* [@Model.Group?.GroupSubjects?.FirstOrDefault(x=>x.GroupId==Model.Group.Id)?.Subject.Name] *@</a>
                        </li>
                    </ul>
                </li>
            </ul>
        </div>
    </div>
</div>